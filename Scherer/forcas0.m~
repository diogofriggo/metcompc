function forcas0(l)
    fx = zeros(1,N);
    fy = zeros(1,N);
    fpx = zeros(1,N);
    fpy = zeros(1,N);
    for k=1:N-1 %1..9
        p = k+1:N; %2..10, 3..11, 4..12, 5..13, 6..14, 7..15, 8..16, 9..17 #THIS IS WRONG
        delx = x(k)-x(p);
        dely = y(k)-y(p);
        R2 = delx.^2+dely.^2;
        list = find(R2<l^2);
        %!isempty(list)
        if(length(list) > 0)
            delx = x(k)-x()
        end
        
        
    end
    
end